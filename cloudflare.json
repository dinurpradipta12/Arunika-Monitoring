{
  "name": "arunika-monitoring",
  "description": "Arunika Monitoring Dashboard",
  "repository": "https://github.com/dinurpradipta12/Arunika-Monitoring",
  "build": {
    "commands": ["npm install", "npm run build"],
    "cwd": ".",
    "include": ["dist/**/*"],
    "exclude": ["node_modules/**/*", ".git/**/*"]
  },
  "env": {
    "NODE_VERSION": "18",
    "NPM_VERSION": "10"
  },
  "routes": [
    {
      "pattern": "/*",
      "file": "index.html",
      "status": 200
    }
  ]
}
